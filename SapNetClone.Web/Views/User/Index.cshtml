@model IEnumerable<ListUserVM>
<div class="row">
	<div class="col-xxl-12">
		<div class="card card-xxl-stretch mb-5">
			<div class="card-header">
				<h3 class="card-title align-items-start flex-column">
					Users
				</h3>
			</div>
			<div class="card-body py-3">
				<table class="table table-striped table-row-bordered gy-5 dt-init" id="postTable">
					<thead>
						<tr class="fw-bolder fs-6 text-gray-700">
							<th>
								Options
							</th>
							<th>
								@Html.DisplayNameFor(model=>model.SocialSecurityCode)
							</th>
							<th>
								@Html.DisplayNameFor(model=>model.Name)
							</th>
							<th>
								@Html.DisplayNameFor(model =>model.Email)
							</th>
							<th>
								@Html.DisplayNameFor(model=>model.TelephoneNumber)
							</th>

						</tr>
					</thead>
					<tbody>
						@foreach (var item in Model)
						{
							var id = item.Id;
							<tr id = "@id">
								<td>
									<a class="btn btn-secondary" asp-action="Detail" asp-route-id="@item.Id">Detail</a>
									<a class="btn btn-info" asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                                    <button class="btn btn-danger" id="deleteButton" onclick="openDeleteDialog('@id')">Delete</button>
								</td>
								<td>
									@Html.DisplayFor(modelItem=>item.SocialSecurityCode)
								</td>
								<td>
									@Html.DisplayFor(modelItem=> item.Name)
								</td>
								<td>
									@Html.DisplayFor(modelItem=>item.Email)
								</td>
								<td>
									@Html.DisplayFor(modelItem=>item.Email)
								</td>
							</tr>
						}
					</tbody>
				</table>
			</div>
		</div>
	</div>

	@section Scripts {
	
	}
